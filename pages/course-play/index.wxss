.play_prompt{float: left;width: 700rpx; background-color: #fff;padding: 10rpx 25rpx;font-size: 22rpx;}
.check{
  float: left;
        width: 33rpx;
        height: 33rpx;
        background-color: #469dff;
        position: relative;
        border-radius: 50%;
        cursor: pointer;
        margin-right: 13rpx;
    }
    .check:after{
        content: '';
        display: block;
        width: 17rpx;
        height: 10rpx;
        border: 3rpx solid #fff;
        border-width: 0 0 5rpx 5rpx;
        position: absolute;
        top: 60%;
        left: 58%;
        margin-top: -12rpx;
        margin-left: -12rpx;
        transform: rotate(-45deg);
    }

