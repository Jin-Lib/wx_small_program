.popup {
  width: 655rpx;
  background-color: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 30rpx 50rpx;
  padding-bottom: 0rpx;
  animation: popups .4s;
}

.popup_title {
  font-size: 30rpx;
  font-weight: 900;
  margin-left: 220rpx;
  border-bottom: solid 5rpx #fc4850;
  width: 200rpx;
  text-align: center;
  padding-bottom: 10rpx;
  margin-bottom:30rpx;
}
.beizhus{float: left;margin-bottom: 120rpx;font-size: 24rpx;color: #999999;margin-top: 15rpx;
  padding: 10rpx 20rpx;}
.close {
  position: absolute;
  right: 15rpx;
  top: 30rpx;
  width: 50rpx;
  height: 50rpx;
}

.close:before, .close:after {
  position: absolute;
  right: 30rpx;
  content: ' ';
  height: 40rpx;
  width: 2px;
  background-color: #333;
}

.close:before {
  transform: rotate(45deg);
}

.close:after {
  transform: rotate(-45deg);
}

.popup_detailed {
  padding: 10rpx 20rpx;
  float: left;
}

.popup_text_title {
  font-size: 24rpx;
  color: #000;
}

.popup_text_title_26 {
  font-size: 26rpx;
  color: #000;
}

.popup_text_title_26 i {
  font-weight: 900;
}

.popup_text_deta {
  font-size: 26rpx;
  color: #6b6b6b;
  float: left;
  margin-top: 15rpx;
  margin-left: 40rpx;
}

@keyframes popups{
  0%{
    bottom:-500rpx;
  }
  100%{
    bottom:0rpx;
  }
}